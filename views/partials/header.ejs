<header id="top-header">
    <span class="siteName">
        <a href="/" title="Return to home page">CSE Motors</a>
    </span>
    <div id="tools">

        <% if (locals.loggedin) { %>
            <a href="/account/logged">Welcome <%- locals.accountData.account_firstname %> </a>
            <form action="/" method='post'>
                <input type="submit" name="logOut" value="Log Out"> 
            </form>
        <% } else { %>
            <a title="Click To Login" href="/account/login">My Account</a>
        <% } %>

    </div>
    </header>